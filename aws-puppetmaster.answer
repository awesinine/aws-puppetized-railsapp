#uncomment everything
#replace REDACTED with passwords
#replace {aws internal name} with the aws internal name :-D
q_activity_database_name=pe-activity
#q_activity_database_password=REDACTED
q_activity_database_user=pe-activity
q_all_in_one_install=y
q_backup_and_purge_old_database_directory=n
q_classifier_database_name=pe-classifier
#q_classifier_database_password=REDACTED
q_classifier_database_user=pe-classifier
q_database_host=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_database_install=y
q_database_port=5432
#q_database_root_password=REDACTED
q_database_root_user=pe-postgres
q_fail_on_unsuccessful_master_lookup=y
q_install=y
q_pe_check_for_updates=y
q_pe_database=y
q_public_hostname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppet_cloud_install=y
#q_puppet_enterpriseconsole_auth_password=REDACTED
q_puppet_enterpriseconsole_httpd_port=443
q_puppet_enterpriseconsole_install=y
q_puppet_enterpriseconsole_master_hostname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetagent_certname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetagent_install=y
q_puppetagent_server=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetca_hostname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetdb_database_name=pe-puppetdb
#q_puppetdb_database_password=REDACTED
q_puppetdb_database_user=pe-puppetdb
q_puppetdb_hostname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetdb_install=y
q_puppetdb_plaintext_port=8080
q_puppetdb_port=8081
q_puppetmaster_certname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetmaster_dnsaltnames={aws internal name},`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetmaster_enterpriseconsole_certname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetmaster_enterpriseconsole_hostname=`curl http://169.254.169.254/latest/meta-data/public-hostname`
q_puppetmaster_enterpriseconsole_port=443
q_puppetmaster_install=y
q_rbac_database_name=pe-rbac
#q_rbac_database_password=REDACTED
q_rbac_database_user=pe-rbac
q_run_updtvpkg=n
q_skip_backup=y
q_skip_master_verification=n
q_vendor_packages_install=y
